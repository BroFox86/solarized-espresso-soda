    "editorGroup.background": "#FFFBF2",
    "editorGroupHeader.tabsBackground": "#D9D2C2",
    "editorGroup.border": "#DDD6C1",
    "editorGroup.dropBackground": "#DDD6C1AA",
    "panel.border": "#DDD6C1",
    "statusBar.foreground": "#586E75",
    "statusBar.background": "#EEE8D5",
    "statusBar.noFolderBackground": "#EEE8D5",
    "statusBarItem.prominentBackground": "#DDD6C1",
    "statusBarItem.prominentHoverBackground": "#DDD6C199",
    "activityBar.background": "#DDD6C1",
    "activityBar.foreground": "#584C27",
    "activityBar.dropBackground": "#EEE8D5",
    "activityBarBadge.background": "#B58900",
    "sideBar.background": "#EEE8D5",
    "sideBar.foreground": "#000000",
    "sideBarTitle.foreground": "#586E75",
    "titleBar.activeBackground": "#EEE8D5",
    "notification.background": "#999178",
    "debugExceptionWidget.border": "#AB395B",
    "debugExceptionWidget.background": "#DDD6C1",
    "debugToolBar.background": "#DDD6C1",
    "statusBar.debuggingBackground": "#EEE8D5",
    "extensionButton.prominentBackground": "#B58900",
    "extensionButton.prominentHoverBackground": "#584C27AA",
    "terminal.ansiBlack": "#073642",
    "terminal.ansiRed": "#DC322F",
    "terminal.ansiGreen": "#859900",
    "terminal.ansiYellow": "#B58900",
    "terminal.ansiBlue": "#268BD2",
    "terminal.ansiMagenta": "#D33682",
    "terminal.ansiCyan": "#2AA198",
    "terminal.ansiWhite": "#7F6D34",
    "terminal.ansiBrightBlack": "#586E75",
    "terminal.ansiBrightRed": "#CB4B16",
    "terminal.ansiBrightGreen": "#586E75",
    "terminal.ansiBrightYellow": "#657B83",
    "terminal.ansiBrightBlue": "#839496",
    "terminal.ansiBrightMagenta": "#6C71C4",
    "terminal.ansiBrightCyan": "#93A1A1",
    "terminal.ansiBrightWhite": "#7F6D34"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#000000",
        "selectionBorder": "#AACBDF",
        "findHighlight": "#FFE792",
        "findHighlightForeground": "#000000"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "comment punctuation"],
      "settings": {
        "foreground": "#93A1A1"
      }
    },
    {
      "name": "Comments: Preprocessor",
      "scope": "comment.block.preprocessor",
      "settings": {
        "foreground": "#ADADAD"
      }
    },
    {
      "name": "Comments: Documentation",
      "scope": ["comment.documentation", "comment.block.documentation"],
      "settings": {
        "foreground": "#BC670F"
      }
    },
    {
      "name": "Invalid - Deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic underline"
      }
    },
    {
      "name": "Invalid - Illegal",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#626FC9"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#61862F"
      }
    },
    {
      "name": "Types",
      "scope": ["storage.type", "support.type"],
      "settings": {
        "foreground": "#6700B9"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language", "support.constant", "variable.language"],
      "settings": {
        "foreground": "#7653C1"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "support.variable",
        "meta.at-rule.include.scss",
        "punctuation.definition.variable.less"
      ],
      "settings": {
        "foreground": "#4C8FC7"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function", "entity"],
      "settings": {
        "foreground": "#61862F"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "foreground": "#3A1D72"
      }
    },
    {
      "name": "Exceptions",
      "scope": "entity.name.exception",
      "settings": {
        "foreground": "#F93232"
      }
    },
    {
      "name": "Sections",
      "scope": "entity.name.section",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant"],
      "settings": {
        "foreground": "#7653C1"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "keyword.operator.attribute-selector.less"],
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Strings",
      "scope": ["constant.character", "string", "meta.attribute-selector.scss"],
      "settings": {
        "foreground": "#BC670F"
      }
    },
    {
      "name": "Strings: Punctuation",
      "scope": "string punctuation",
      "settings": {
        "foreground": "#E69A4C"
      }
    },
    {
      "name": "Strings: Escape Sequences",
      "scope": "constant.character.escape",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings: Regular Expressions",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#699D36"
      }
    },
    {
      "name": "Strings: Symbols",
      "scope": "constant.other.symbol",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Embedded Source",
      "scope": ["string source", "text source"],
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Markup: Changed",
      "scope": "markup.changed",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markup: Deletion",
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup: Error",
      "scope": "markup.error",
      "settings": {
        "foreground": "#F9F2CE"
      }
    },
    {
      "name": "Markup: Insertion",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "Markup: Output",
      "scope": ["markup.output", "markup.raw"],
      "settings": {
        "foreground": "#7F7F7F"
      }
    },
    {
      "name": "Markup: Prompt",
      "scope": "markup.prompt",
      "settings": {
        "foreground": "#555555"
      }
    },
    {
      "name": "Markup: Heading",
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Strong",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Traceback",
      "scope": "markup.traceback",
      "settings": {
        "foreground": "#F93232"
      }
    },
    {
      "name": "Markup: Underline",
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Extra: Diff Range",
      "scope": ["meta.diff.range", "meta.diff.index", "meta.separator"],
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff From",
      "scope": "meta.diff.header.from-file",
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "Extra: Diff To",
      "scope": "meta.diff.header.to-file",
      "settings": {
        "foreground": "#434343"
      }
    },
    {
      "name": "HTML: Doctype Declaration",
      "scope": "meta.tag.sgml.doctype",
      "settings": {
        "foreground": "#7F7F7F"
      }
    },
    {
      "name": "HTML: Tags",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#2F6F9F"
      }
    },
    {
      "name": "HTML: Attribute Punctuation",
      "scope": "meta.tag string punctuation",
      "settings": {
        "foreground": "#5FAFEF"
      }
    },
    {
      "name": "HTML: Tag Punctuation",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#4F9FCF"
      }
    },
    {
      "name": "HTML: Entities",
      "scope": "constant.character.entity",
      "settings": {
        "foreground": "#000000"
      }
    },
    {
      "name": "HTML: Attribute Names",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#4C8FC7"
      }
    },
    {
      "name": "HTML: Attribute Values",
      "scope": [
        "meta.tag string.quoted",
        "meta.tag string.quoted constant.character.entity"
      ],
      "settings": {
        "foreground": "#D44950"
      }
    },

    {
      "name": "PUG: Strings",
      "scope": "text.pug meta.tag string.quoted",
      "settings": {
        "foreground": "#BC670F"
      }
    },
    {
      "name": "PUG: Tags",
      "scope": "entity.name.tag.pug",
      "settings": {
        "foreground": "#61862F"
      }
    },
    {
      "name": "PUG: Attributes",
      "scope": "entity.other.attribute-name.tag.pug",
      "settings": {
        "foreground": "#2F6F9F"
      }
    },
    {
      "name": "PUG: Mixins",
      "scope": "entity.name.function.pug",
      "settings": {
        "foreground": "#9C27B0"
      }
    },
    {
      "name": "PUG: Classes",
      "scope": "entity.other.attribute-name.class.pug",
      "settings": {
        "foreground": "#4F9FCF"
      }
    },
    {
      "name": "PUG: Variables",
      "scope": [
        "text.pug variable.other.readwrite.js, variable.control.import.include.pug"
      ],
      "settings": {
        "foreground": "#268BD2"
      }
    },
    {
      "name": "PUG: Buffered comments",
      "scope": "string.comment.buffered.block.pug",
      "settings": {
        "foreground": "#777"
      }
    },
    {
      "name": "CSS: Property Name",
      "scope": ["meta.property-name", "support.type.property-name"],
      "settings": {
        "foreground": "#D4430D"
      }
    },
    {
      "name": "CSS: Selectors",
      "scope": [
        "meta.selector",
        "meta.selector entity",
        "meta.selector entity punctuation"
      ],
      "settings": {
        "foreground": "#3A77BF"
      }
    },
    {
      "name": "CSS: Classes",
      "scope": [
        "entity.other.attribute-name",
        "entity.other.attribute-name.parent-selector-suffix.css punctuation.definition.entity.css"
      ],
      "settings": {
        "foreground": "#4F9FCF"
      }
    },
    {
      "name": "CSS: HEX color value",
      "scope": [
        "constant.other.color.rgb-value.hex.css",
        "constant.other.color.rgb-value.hex.css punctuation.definition.constant.css"
      ],
      "settings": {
        "foreground": "#43A202"
      }
    },
    {
      "name": "CSS/LESS: Tags",
      "scope": ["entity.name.tag.css", "entity.name.tag.less"],
      "settings": {
        "foreground": "#2F6F9F"
      }
    },
    {
      "name": "CSS/SCSS/LESS: Property Values",
      "scope": [
        "meta.property-value constant.numeric",
        "meta.property-value constant",
        "meta.property-value keyword",
        "keyword.other.unit.px.css",
        "keyword.other.unit.s.css",
        "constant.numeric.css",
        "meta.definition.variable.scss keyword.other.unit.percentage.css",
        "constant.numeric.less",
        "constant.other.color.rgb-value.less punctuation.definition.constant.less"
      ],
      "settings": {
        "foreground": "#43A202"
      }
    },
    {
      "name": "CSS/LESS: Class name dots",
      "scope": [
        "entity.other.attribute-name.class.css punctuation.definition.entity.css",
        "entity.other.attribute-name.class.less punctuation.definition.entity.less"
      ],
      "settings": {
        "foreground": "#4C8FC7"
      }
    },
    {
      "name": "CSS/LESS: Pseudo elements",
      "scope": [
        "entity.other.attribute-name.pseudo-element.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.less",
        "entity.other.attribute-name.pseudo-class.less"
      ],
      "settings": {
        "foreground": "#4C8FC7"
      }
    },
    {
      "name": "CSS/LESS: Pseudo elements punctuation",
      "scope": [
        "punctuation.definition.entity.css",
        "punctuation.definition.entity.less"
      ],
      "settings": {
        "foreground": "#4C8FC7"
      }
    },
    {
      "name": "CSS/SCSS/LESS: Functions",
      "scope": [
        "support.function.misc.css",
        "support.function.misc.scss",
        "support.function.transform.css",
        "support.function.transform.less",
        "support.function.gradient.css",
        "support.function.gradient.less",
        "entity.name.function.scss",
        "support.function.color.less"
      ],
      "settings": {
        "foreground": "#61862F"
      }
    },
    {
      "name": "CSS/SCSS/LESS: At (@) prefixes",
      "scope": [
        "punctuation.definition.keyword.css",
        "punctuation.definition.keyword.scss",
        "punctuation.definition.keyword.less"
      ],
      "settings": {
        "foreground": "#61862F"
      }
    },
    {
      "name": "CSS/SCSS/LESS: Important values",
      "scope": [
        "keyword.other.important.css",
        "keyword.other.important.scss",
        "keyword.other.important.less",
        "keyword.other.important.less punctuation.separator.less"
      ],
      "settings": {
        "foreground": "#D33682"
      }
    },
    {
      "name": "SCSS: Extend values",
      "scope": [
        "meta.at-rule.extend.scss",
        "meta.at-rule.extend.scss support.constant.mathematical-symbols.scss"
      ],
      "settings": {
        "foreground": "#4F9FCF"
      }
    },
    {
      "name": "SCSS: Placeholder (%) prefixes",
      "scope":
        "source.css.scss entity.other.attribute-name.placeholder.css punctuation.definition.entity.css",
      "settings": {
        "foreground": "#4F9FCF"
      }
    },
    {
      "name": "LESS: Function call values",
      "scope": "meta.function-call.less",
      "settings": {
        "foreground": "#7653C1"
      }
    },
    {
      "name": "LESS: Pseudo element keywords",
      "scope": "keyword.other.pseudo-class.less",
      "settings": {
        "foreground": "#7653C1"
      }
    }
  ]
}
